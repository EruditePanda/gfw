<div class="widget">
  <div class="m-widget-tree-cover-loss-ranking">
    <div class="ranking">
      <div class="list">
        <h3 class="heading">{{titleText}}</h3>
        {{#each data}}
          <div class="item">
            <div class="number">{{index}}</div>
            <div class="name">{{data.name}}</div>
            <div class="value{{#if data.selected }} -selected{{/if}}">{{data.value}}<span class="unit">Mha</span></div>
          </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
